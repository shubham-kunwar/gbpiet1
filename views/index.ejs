        <input class="" name="passwordme" type="text" placeholder="Enter Secret Key">
        <button class="btn btn-primary" type="submit">Next</button>
        </form>        
      </div>
      <div class=" row container mx-auto mt-3">
        <form id="login-box" method="POST" action="/ee" role="form"
        class="form-horizontal  border border-info rounded p-2">
        <h3>Electrical</h3>
        <label for="passwordee"></label>
        <input class="" name="passwordee" type="text" placeholder="Enter Secret Key">
        <button class="btn btn-primary" type="submit">Next</button>
        </form>        
      </div>
      <div class=" row container mx-auto mt-3">
        <form id="login-box" method="POST" action="/ce" role="form"
        class="form-horizontal  border border-info rounded p-2">
        <h3>Civil</h3>
        <label for="passwordce"></label>
        <input class="" name="passwordcee" type="text" placeholder="Enter Secret Key">
        <button class="btn btn-primary" type="submit">Next</button>
        </form>        
      </div>
      <div class=" row container mx-auto mt-3">
        <form id="login-box" method="POST" action="/ece" role="form"
        class="form-horizontal  border border-info rounded p-2">
        <h3>Electronics</h3>
        <label for="passwordece"></label>
        <input class="" name="passwordece" type="text" placeholder="Enter Secret Key">
        <button class="btn btn-primary" type="submit">Next</button>
        </form>        
      </div>
      
     
    </div>
    
  </body>
</html>
